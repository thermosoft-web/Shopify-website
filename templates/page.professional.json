    "multicolumn_with_icons_UKXGaU": {
      "type": "multicolumn-with-icons",
      "blocks": {
        "column_T7TDUB": {
          "type": "column",
          "settings": {
            "image": "shopify://shop_images/Group.svg",
            "title": "Customer Referrals",
            "text": "<p>You will be eligible to receive referrals to projects in your area. Let us save you time by bringing the business to you! We strive to match our end users with trained professionals to ensure their project goes quickly and smoothly.</p>",
            "link_label": "",
            "link": ""
